{
  "name": "simplified-fetch",
  "version": "0.0.9",
  "description": "Encapsulate a unified API request object to simplify the use of fetch",
  "main": "bin/index.umd.js",
  "module": "bin/index.esm.js",
  "typings": "bin/simplified-fetch.d.ts",
  "scripts": {
    "build": "rollup -c rollup.prod.ts --watch",
    "doc": "api-extractor run --local --verbose && api-documenter markdown -i temp -o docs",
    "test": "jest --watchAll",
    "prepub": "rollup -c rollup.prod.ts && jest --no-cache & npm run doc"
  },
  "homepage": "https://github.com/Benno-Wu/SimplifiedFetch#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Benno-Wu/SimplifiedFetch.git"
  },
  "keywords": [
    "fetch",
    "promise",
    "api",
    "ajax",
    "browser"
  ],
  "files": [
    "bin"
  ],
  "author": "BennoWu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Benno-Wu/SimplifiedFetch/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.2.0",
    "jest": "^26.6.3",
    "puppeteer-core": "^8.0.0",
    "rollup": "^2.41.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-uglify": "^6.0.4",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  }
}
